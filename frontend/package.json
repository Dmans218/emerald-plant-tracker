{
  "name": "emerald-plant-tracker-frontend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "axios": "^1.10.0",
    "chart.js": "^4.5.0",
    "date-fns": "^4.1.0",
    "exif-js": "^2.3.0",
    "globals": "^16.2.0",
    "lucide-react": "^0.513.0",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.54.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^7.6.2",
    "recharts": "^2.13.3",
    "tesseract.js": "^6.0.1",
    "web-vitals": "^5.0.2"
  },
  "scripts": {
    "dev": "bun run build && bun run serve.js",
    "start": "bun run build && bun run serve.js",
    "build": "bun build src/index.js --outdir=build --target=browser --minify --sourcemap=external --define 'process.env.NODE_ENV=\"development\"' --define 'process.env={}' --define 'global=globalThis'",
    "serve": "bun run serve.js",
    "test": "bun test",
    "lint": "bun run eslint src/",
    "lint:fix": "bun run eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\""
  },
  "npmConfig": {
    "legacy-peer-deps": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "eslint": "^9.28.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.3.3"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "overrides": {
    "nth-check": "^2.1.1",
    "postcss": "^8.4.31",
    "eslint": "^9.28.0",
    "eslint-webpack-plugin": "^4.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint-config-react-app": "^7.1.0-next.26",
    "brace-expansion": "^2.0.1",
    "minimatch": "^9.0.0",
    "glob": "^10.0.0",
    "tailwindcss": "^4.1.8",
    "webpack-dev-server": "^5.2.2"
  }
}
